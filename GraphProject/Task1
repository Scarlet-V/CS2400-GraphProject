
import java.util.*;

class Graph<E> 
{
    private Map<E, List<E> > edgeMap;
	 
	Graph()
    {
    edgeMap = new HashMap<>();
	}

    public void addVertex(E s)
    {
        edgeMap.put(s, new LinkedList<E>());
    }
    public void addEdge(E from, E to)
    {  
        if (!edgeMap.containsKey(from))
            addVertex(from);
  
        if (!edgeMap.containsKey(to))
            addVertex(to);
  
        edgeMap.get(from).add(to);
    }


}